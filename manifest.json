  {
    "name": "Custom Youtube",
    "version": "1.0",
    "description": "Orders Youtube videos by duration, among other stuff",
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Custom Youtube"
    },
    "manifest_version": 2,
    "permissions": [
      "https://www.googleapis.com/",
      "https://i.ytimg.com/",
      "storage"
    ],
    "icons": {
      "128": "icon128.png"
    }
  }